  file = {Attachment:/home/martin/text_stud-zotero/storage/HJ7ANUUE/Kieler et al. - 2007 - IEEE Transactions on Applied Superconductivity - SNS Josephson Junction Series Arrays for the Josephson Arbitrary.pdf:application/pdf}

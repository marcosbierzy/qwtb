  file = {Attachment:/home/martin/text_stud-zotero/storage/GGVFQTFP/Eklund et al. - 2011 - IEEE Transactions on Instrumentation and Measurement - Determination of Transition Error Corrections for Low Freq.pdf:application/pdf}
}


@article{Streit2010c,
  title = {Bilateral comparison of 100 {pF} capacitance standards (ongoing {BIPM} key comparison {BIPM}.{EM}-K14.b) between the {CMI}, Czech Republic and the {BIPM}, January?July 2009},
  volume = {47},
  issn = {0026-1394},
  url = {http://iopscience.iop.org/0026-1394/47/1A/01014},

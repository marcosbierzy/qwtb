  journaltitle = {{IEEE} Transactions on Instrumentation and Measurement},
  shortjournal = {{IEEE} Trans. Instrum. Meas.},
  author = {Kieler, O.F.O. and Landim, R.P. and Benz, S.P. and Dresselhaus, P. D. and Burroughs, C. J.},
  date = {2008-04},
  pages = {791--796},
  keywords = {electro,jvs,jvs.ac,metrology,voltage},
  file = {Attachment:/home/martin/text_stud-zotero/storage/RR9QAU63/Kieler et al. - 2008 - IEEE Transactions on Instrumentation and Measurement - AC–DC Transfer Standard Measurements and Generalized Com.pdf:application/pdf}
}

